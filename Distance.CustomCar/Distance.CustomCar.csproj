GOTO :EOF

:COPYDIR
XCOPY /i /s /Y /v %251 %252
GOTO :EOF

:INCLUDEGSL
CALL :MAKEDIR "%25PACKAGES%25\GameSupport"
CALL :COPY "%25BUILD%25%251.dll" "%25PACKAGES%25\GameSupport"
GOTO :EOF

:INCLUDEDEPENDENCY
